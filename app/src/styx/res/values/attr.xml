<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="autoCompleteTitleColor" format="reference|color" />
    <attr name="autoCompleteUrlColor" format="reference|color" />
    <attr name="searchBackground" format="reference|color" />
    <attr name="appColorControlEnabled" format="reference|color" />
    <attr name="appColorControlDisabled" format="reference|color" />
    <attr name="appColorControlSelected" format="reference|color" />

    <declare-styleable name="TabCountView">
        <attr name="tabIconColor" format="reference|color" />
        <attr name="tabIconTextSize" format="dimension"/>
        <attr name="tabIconBorderWidth" format="dimension"/>
        <attr name="tabIconBorderRadius" format="dimension"/>
    </declare-styleable>

    <!-- Progress Bar -->
    <declare-styleable name="AnimatedProgressBar">
        <attr name="progressColor" format="color" />
        <attr name="bidirectionalAnimate" format="boolean" />
        <attr name="animationDuration" format="integer" />
    </declare-styleable>
</resources>
